{
  "name": "polished",
  "author": "Ryan Wilson",
  "description": "The Polished Writing App",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "env-cmd -f ./env/.env.local react-scripts start",
    "build": "react-scripts build",
    "build:preflight": "env-cmd -f ./env/.env.preflight npm run build",
    "build:prod": "env-cmd -f ./env/.env.prod npm run build",
    "build:prod:electron": "env-cmd -f ./env/.env.electron.prod npm run build",
    "test": "react-scripts test",
    "test:ci": "npm run test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/react": "^1.6.6",
    "@statickit/react": "^2.1.1",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.7.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-stripe-elements": "^6.0.4",
    "@types/theme-ui": "^0.3.4",
    "bowser": "^2.9.0",
    "custom-electron-titlebar": "^3.2.2",
    "deepmerge": "^4.2.2",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.2",
    "electron-settings": "^4.0.2",
    "electron-updater": "^4.3.1",
    "firebase": "^7.15.5",
    "flesch-kincaid": "^1.0.5",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "mobx-state-tree": "^3.16.0",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
